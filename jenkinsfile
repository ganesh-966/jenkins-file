pipeline {
    agent any

    stages {
        stage('pull') {
            steps {
                git branch: 'main', url: 'https://github.com/ganesh-966/mirror-repo-project.git'
            }
        }
        stage('build') {
            steps {
                echo 'build job'
            }
        }
        stage('test') {
            steps {
                echo 'test job'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy job'
            }
        }
    }
}